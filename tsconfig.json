{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@content/*": [
        "content/*"
      ],
      "@data/*": [
        "data/*"
      ],
      "@graphql/*": [
        "graphql/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@models/*": [
        "models/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@scripts/*": [
        "scripts/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}